<div id="editor">
  <button id="select-image">选择图片</button>
  <input type="file" capture="camera" accept="image/*" id="image-field">
</div>
<div id="image-palette">
  <img id="target-image">
  <div id="colors-palette">
    <div id="color-1" class="palette"></div>
    <div id="color-2" class="palette"></div>
    <div id="color-3" class="palette"></div>
    <div id="color-4" class="palette"></div>
    <div id="color-5" class="palette"></div>
  </div>
</div>